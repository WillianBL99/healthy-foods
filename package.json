{
  "name": "healthy-foods-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "ttsc -p tsconfig.json && rm -rf dist/test",
    "dev": "npm run dev:load-envs nodemon",
    "dev:load-envs": "dotenv -e .env.development",
    "docker:build": "npm run docker:load-envs ttsc -p tsconfig.json",
    "docker:start": "npm run docker:load-envs node dist/src/server.js",
    "docker": "npm run docker:load-envs npm run docker:compose up --build",
    "docker:down": "npm run docker:load-envs npm run docker:compose down",
    "docker:compose": "docker-compose -f docker-compose.yml",
    "docker:load-envs": "dotenv -e .env.docker",
    "prod:load-envs": "dotenv -e .env.production",
    "test": "npm run test:load-envs vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "npm run test:load-envs vitest",
    "start": "npm run prod:load-envs node dist/src/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.3",
    "joi": "^17.7.0",
    "mongodb": "^4.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.24.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.4.4",
    "vitest": "^0.24.4"
  }
